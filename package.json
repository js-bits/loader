{
  "name": "@js-bits/loader",
  "version": "1.0.7",
  "description": "HTTP resource loader",
  "keywords": [
    "javascript",
    "typescript",
    "http",
    "request",
    "response",
    "executor",
    "pattern",
    "loader",
    "fetch",
    "promise",
    "timing",
    "latency",
    "measurement"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./index.js"
    }
  },
  "scripts": {
    "build": "rimraf ./dist && yarn build:dts && rollup --config",
    "build:dts": "tsc ./index.js --allowJs --emitDeclarationOnly --declaration --esModuleInterop --outDir dist",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "test-cjs": "jest --verbose",
    "test-coverage": "yarn test --coverage",
    "test-watch": "yarn test --watch",
    "lint": "tsc --noEmit && eslint '**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/js-bits/loader"
  },
  "author": "https://github.com/apolo-gh",
  "license": "ISC",
  "dependencies": {
    "@js-bits/dom-parser": "^1.0.2",
    "@js-bits/enumerate": "^1.0.17",
    "@js-bits/executor": "^2.0.11",
    "@js-bits/fetch": "^3.1.2",
    "@js-bits/timeout": "^1.0.12"
  },
  "devDependencies": {
    "@js-bits/formalinter": "^1.0.3",
    "@js-bits/log-in-color": "^1.0.2",
    "@rollup/plugin-virtual": "^3.0.1",
    "@types/jest": "29.4.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.24.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14.18"
  },
  "prettier": "@js-bits/formalinter/.prettierrc.json",
  "eslintConfig": {
    "extends": "@js-bits/eslint-config/.eslintrc.json"
  }
}
